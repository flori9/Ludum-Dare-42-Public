    if (mouse_check_button_pressed(mb_left)) {
        var current_upgrade_option = upgrading_get_current_option();
        if (current_upgrade_option &gt;= 0) {
            var cost = tile_get_info(current_upgrade_option);
            if (can_pay_cost(cost)) {
                pay_cost(cost);
                if (current_upgrade_option &gt;= 0) {
                    upgrading_perform(current_upgrade_option);
                    fadeoutred = 1;
                    fadeoutcolor = c_white;
                }
            } else { 
                fadeoutred = 1;
                fadeoutcolor = obj_ui.textcolorred;
            }
            ds_map_destroy(cost);
        } else {
            fadeoutred = 1;
            fadeoutcolor = c_ltgray;
        }
    }
}
if (fadeoutred &gt; 0)
    fadeoutred -= 0.05;
else
    fadeoutred = 0;
    
if (fadeout &gt; 0)
    fadeout -= 0.05;
